@prefix sp: <http://spinrdf.org/sp#>.
@prefix spin: <http://spinrdf.org/spin#>.
@prefix spl: <http://spinrdf.org/spl#>.
@prefix arg: <http://spinrdf.org/arg#>.
@prefix ex: <ex:>.

ex:Person a rdfs:Class ;
		spin:constraint [
				a spin:Templates, spin:ConstructTemplates, spl:PropertyConstraintTemplates, spl:CountPropertyConstraintTemplates, spl:ObjectCountPropertyConstraint ;
				arg:property ex:prop ;
				arg:minCount 1 ;
				rdfs:label """Test constraint"""
		] .

ex:Tom a ex:Person .
ex:Tom ex:age 43 .

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dct: <http://purl.org/dc/terms/>

SELECT ?lang (COUNT(*) AS ?c)
WHERE {
    ?dataset a dcat:Dataset ;
             dct:language ?lang .
    OPTIONAL { ?dataset dcat:distribution ?dist . ?dist dcat:accessURL ?url }
    FILTER (!isBlank(?lang))
}
GROUP BY ?lang
HAVING (?c > 10)

                             sh:maxCount 1 ;
                             # sh:severity   sh:Violation
                     ],
                     [
                             sh:path  dct:title ;
                             sh:nodeKind sh:Literal;
                             sh:maxCount 1 ;
                             # sh:severity   sh:Violation
                     ],
                     [
                             sh:path  dct:description ;
                             sh:nodeKind sh:Literal;
                             # sh:severity   sh:Violation
                     ],
                     [
                             sh:path  dct:description ;
                             sh:minCount 1 ;
                             # sh:severity   sh:Warning
                     ].
  []
                    a              sh:NodeShape ;
                    sh:targetClass  spdx:Checksum ;
                    sh:property

                            sh:maxCount   1 ;
                            sh:minCount   1 ;
                            sh:path spdx:algorithm ;
                            # sh:in (spdx:checksumAlgorithm_md5 spdx:checksumAlgorithm_sha1 spdx:checksumAlgorithm_sha256) ;
                            # sh:severity   sh:Violation

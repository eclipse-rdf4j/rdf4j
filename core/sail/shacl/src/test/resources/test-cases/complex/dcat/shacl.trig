                     ],
                     [
                             sh:class   skos:Concept ;
                             sh:path  adms:status ;
                             sh:maxCount 1;
                             # sh:severity   sh:Violation
                     ],
                     [
                                  sh:path  dcat:accessURL ;
                             sh:nodeKind sh:IRI ;

                             # sh:severity   sh:Violation


                     ],

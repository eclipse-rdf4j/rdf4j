@prefix ex: <http://example.com/ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsx: <http://rdf4j.org/shacl-extensions#> .
@prefix rdf4j: <http://rdf4j.org/schema/rdf4j#> .

[] a sh:ValidationReport;
  rdf4j:truncated false;
  sh:conforms false;
  sh:result [ a sh:ValidationResult;
      rsx:shapesGraph rdf4j:SHACLShapeGraph;
      sh:focusNode ex:validPerson1;
      sh:resultPath ex:iriOr10LengthEnglishNorwegianLiteral;
      sh:resultSeverity sh:Violation;
      sh:sourceConstraintComponent sh:OrConstraintComponent;
      sh:sourceShape [ a sh:PropertyShape;
          sh:or ([ a sh:NodeShape;
                sh:nodeKind sh:IRI
              ] [ a sh:NodeShape;
                sh:languageIn ("no" "en");
                sh:minLength 10;
                sh:nodeKind sh:Literal
              ]);
          sh:path ex:iriOr10LengthEnglishNorwegianLiteral
        ];
      sh:value 12345678901
    ] .

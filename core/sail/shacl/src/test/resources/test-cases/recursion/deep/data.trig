  ex:node3 ex:next ex:node4 .
  ex:node4 ex:next ex:node5 .
  ex:node5 ex:next ex:node6 .
  ex:node6 ex:next ex:node7 .
  ex:node7 ex:next ex:node8 .
  ex:node8 ex:next ex:node9 .
  ex:node9 ex:next ex:node10 .
  ex:node10 ex:next ex:node11 .
  ex:node11 ex:next ex:node12 .

  ex:node12 ex:value "twelve" .

  ex:friend1 rdf:type ex:Friend .

}
